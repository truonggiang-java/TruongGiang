package lap3_bai1_intamgiac;

import java.util.Scanner;

public class Exercise02 {
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("Nhap n: ");
		int n=sc.nextInt();
		for(int i=1;i<=n;i++) {
			for(int j=1;j<=i;j++) {
				System.out.printf("%d",i);
			}
			System.out.println();
		}
	}
}
